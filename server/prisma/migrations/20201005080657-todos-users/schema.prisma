datasource db {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Users {
  id    Int @default(autoincrement()) @id
  name  String
  todos Todos[]
}

model Todos {
  id        Int  @default(autoincrement()) @id
  text      String
  completed Boolean @default(false)
}
